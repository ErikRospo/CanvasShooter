{
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"files.autoSave": "afterDelay",
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"files.trimFinalNewlines": true,
	"editor.renderControlCharacters": true,
	"editor.renderFinalNewline": false,
	"files.defaultLanguage": "${activeEditorLanguage}",
	"explorer.confirmDelete": false,
	"csharp.suppressDotnetInstallWarning": true,
	"csharp.suppressDotnetRestoreNotification": true,
	"editor.multiCursorModifier": "ctrlCmd",
	"workbench.editor.enablePreview": false,
	"github.copilot.enable": {
		"*": true
	},
	"git.confirmSync": false,
	"git.autofetch": true,
	"files.associations": {
		"*.js": "javascript",
		"*.css": "css",
		"*.json": "jsonc"
	},
	"remoteHub.commitDirectlyWarning": "off",
	"explorer.confirmDragAndDrop": false,
	"instantmarkdown.autoOpenBrowser": false,
	"instantmarkdown.autoCloseServerAndBrowser": false,
	"editor.snippetSuggestions": "top",
	"editor.suggest.preview": true,
	"editor.suggest.showStatusBar": true,
	"security.workspace.trust.enabled": false,
	"editor.formatOnSave": true,
	"git.enableSmartCommit": true,
	"javascript.format.semicolons": "insert",
	"workbench.iconTheme": "material-icon-theme",
	"latex-workshop.view.pdf.viewer": "tab",
	"liveServer.settings.donotVerifyTags": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"[json]": {
		"editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
	},
	"json.format.enable": false,
	"emeraldwalk.runonsave": {
		"commands": [
			{
				"match": "(tailwind.config.js)|(index.css)",
				"cmd": "npx tailwindcss -i index.css -o tailwind.css"
			},
			{
				"match": ".//src//TS//*.ts",
				"cmd": "tsc"
			}
		]
	},
	"todo.symbols.done": "[x]",
	"todo.symbols.cancelled": "[-]",
	"todo.symbols.box": "[ ]",
	"todo.colors.comment": "#90afbf",
	"todo.archive.sortByDate": true,
	"todo.colors.tag": "#82e65e",
	"todo.colors.types": {
		"maybe": "#ccff00",
		"TODO": "#ffcc00",
		"FIXME": "#cc0000",
		"FIX": "#cc0000",
		"BUG": "#cc0000",
		"UGLY": "#cc0000",
		"DEBUG": "#cc0000",
		"HACK": "#cc0000",
		"REVIEW": "#00ccff",
		"OPTIMIZE": "#00ccff",
		"TSC": "#00ccff",
		"NOTE": "#cc00cc",
		"IDEA": "#cc00cc"
	},
	"todo.statistics.statusbar.enabled": "global.all > 0",
	"editor.formatOnSaveMode": "modificationsIfAvailable",
	"files.autoSaveDelay": 2000,
	"window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}${appName}",
	"todo.tags.names": [
		"critical",
		"high",
		"low",
		"today",
		"maybe",
		"ongoing",
		"bugs"
	],
	"todo.timekeeping.created.enabled": true,
	"liveServer.settings.ChromeDebuggingAttachment": true,
	"workbench.startupEditor": "none",
	"html.autoClosingTags": true,
	"html.validate.styles": true,
	"html.validate.scripts": true,
	"html.hover.documentation": true,
	"html.hover.references": true,
	"html.suggest.html5": true,
	"css.hover.documentation": true,
	"editor.fontFamily": "Fira Code, Consolas, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"git.closeDiffOnOperation": true,
	"todo.timekeeping.created.time": true,
	"todo.timekeeping.elapsed.enabled": true,
	"todo.timer.statusbar.enabled": "estimate",
	"explorer.autoReveal": true,
	"scm.defaultViewMode": "tree",
	"scm.diffDecorationsIgnoreTrimWhitespace": "true",
	"files.exclude": {
		"**/Timing": true
	},
	"editor.guides.bracketPairs": true,
	"editor.linkedEditing": true,
	"editor.scrollbar.horizontal": "auto",
	"editor.scrollbar.vertical": "visible",
	"editor.tabCompletion": "on",
	"editor.formatOnPaste": true,
	"editor.formatOnType": true,
	"editor.minimap.showSlider": "always",
	"workbench.view.alwaysShowHeaderActions": false,
	"workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
	"workbench.settings.useSplitJSON": true,
	"explorer.compactFolders": false,
	"explorer.experimental.fileNesting.enabled": true,
	"explorer.incrementalNaming": "smart",
	"scm.alwaysShowActions": true,
	"scm.alwaysShowRepositories": true,
	"terminal.explorerKind": "integrated",
	"terminal.integrated.defaultLocation": "editor",
	"terminal.integrated.defaultProfile.linux": "bash",
	"problems.showCurrentInStatus": true,
	"vsintellicode.features.apiExamples": "enabled",
	"vsintellicode.features.python.deepLearning": "enabled",
	"prettier.useTabs": true,
	"prettier.singleQuote": true,
	"liveServer.settings.ignoreFiles": [
		"!**/node_modules/**",
		".vscode/**",
		"**/*.scss",
		"**/*.sass",
		"**/*.ts",
		"**/readme.md",
		"/temp/**"
	],
	"vscode-json-editor.theme": "dark",
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.enumMemberValues.enabled": true,
	"javascript.preferences.quoteStyle": "single",
	"javascript.referencesCodeLens.enabled": true,
	"javascript.suggest.completeFunctionCalls": true,
	"typescript.format.semicolons": "insert",
	"minify.minifyExistingOnSave": false,

	"minify.js": {
		"mangle": true,
		"compress": {
			"sequences": true,
			"properties": true,
			"dead_code": true,
			"drop_debugger": true,
			"unsafe": false,
			"unsafe_comps": false,
			"conditionals": true,
			"comparisons": true,
			"evaluate": true,
			"booleans": true,
			"loops": true,
			"unused": true,
			"hoist_funs": true,
			"keep_fargs": false,
			"keep_fnames": false,
			"hoist_vars": false,
			"if_return": true,
			"join_vars": true,
			"collapse_vars": false,
			"reduce_vars": true,
			"side_effects": true,
			"pure_getters": false,
			"pure_funcs": null,
			"negate_iife": false,
			"drop_console": true,
			"passes": 1,
			"global_defs": {}
		},
		"output": {
			"ascii_only": false,
			"inline_script": false,
			"max_line_len": 32000,
			"braces": false,
			"semicolons": true,
			"comments": false,
			"shebang": false,
			"preamble": null,
			"quote_style": "best"
		}
	}
}
