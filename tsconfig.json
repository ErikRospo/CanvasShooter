{
  "compilerOptions": {
    "module": "AMD",
    "removeComments": true,
    "noUnusedParameters": true,
    "target": "ES6",
    "sourceMap": true,
    "lib": ["ES2015", "ES2016", "ES2017", "ES2018", "ES2019", "ES2020", "ES2021", "DOM"],
    "moduleResolution": "node",
    "outFile": "./src/JS/compiled.js",
    "declaration": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "inlineSources": true,
    "diagnostics": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
},
  "files": [
    "./src/TS/init.ts",
    "./src/TS/helperFunctions.ts",
    "./src/TS/debug.ts",
    "./src/TS/classes/halo.ts",
    "./src/TS/classes/Health.ts",
    "./src/TS/classes/player.ts",
    "./src/TS/classes/projectile.ts",
    "./src/TS/classes/enemy.ts",
    "./src/TS/classes/particle.ts",
    "./src/TS/classes/highScores.ts",
    "./src/TS/classes/MusicPlay.ts",
    "./src/TS/shop.ts",
    "./src/TS/vars.ts",
    "./src/TS/Events.ts",
    "./src/TS/animate.ts",
    "./src/TS/functions.ts"
  ]
}
